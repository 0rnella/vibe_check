{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-console": "off",
    "no-tabs": 0,
    "indent": ["error", "tab"],
    "linebreak-style": ["error", "windows"]
  }
}
